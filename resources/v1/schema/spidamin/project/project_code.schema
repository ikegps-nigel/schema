{
  "id": "#/spidamin/project/project_code.schema",
  "description": "A ProjectManager member",
  "type": "object",
  "required": [
    "companyId",
    "type",
    "value"
  ],
  "properties": {
    "id": {
      "type": "number",
      "description": "This is a database ID.  Don't set this value for project code creation.  Set this value if you want to update an existing project code."
    },
    "companyId": {
      "type": "number"
    },
    "type": {
      "type": "string",
      "enum": [
        "PROJECT",
        "WORKORDER",
        "NJUNS",
        "AUUID"
      ]
    },
    "value": {
      "type": "string",
      "minLength": 2,
      "maxLength": 72
    }
  },
  "additionalProperties": false
}