{
  "id": "#/spidacalc/cee/job-action-response.schema",
  "description": "The response from a job action (add,update,remove,validate)",
  "type": "object",
  "required": [
    "success"
  ],
  "properties": {
    "id": {
      "description": "If the job is successfully added to the queue, then a unique ID will be generated by CEE",
      "type": "string"
    },
    "success": {
      "description": "true if job action was successful",
      "type": "boolean"
    },
    "errors": {
      "description": "an array of errors",
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "additionalProperties": false
  }
}