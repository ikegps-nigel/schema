{
    "id": "project",
    "description": "A SPIDAMin project",
    "type": "object",
    "properties": {
        "id": {
            "type": "number"
        },
        "name": {
            "type": "string"
        },
        "statusColor": {
            "type": "string"
        },
        "statusName": {
            "type": "string"
        },
        "requiredFilled": {
            "type": "boolean"
        },
        "address": {
            "type": "object",
            "optional": true,
            "description": "project address",
            "properties": {
                "number": {
                    "type": "string",
                    "optional": true
                },
                "street": {
                    "type": "string",
                    "optional": true
                },
                "county": {
                    "type": "string",
                    "optional": true
                },
                "city": {
                    "type": "string",
                    "optional": true
                },
                "state": {
                    "type": "string",
                    "optional": true
                },
                "zip_code": {
                    "type": "string",
                    "optional": true
                }
            }
        },
        "projectCodes": {
            "type": "array",
            "optional": false,
            "description": "Codes for the project.",
            "items": {
                "type": "string"
            }
        },
        "formTemplateIds": {
            "type": "array",
            "optional": false,
            "description": "Form template ids for the project.",
            "items": {
                "type": "number"
            }
        },
        "dataFormIds": {
            "type": "array",
            "optional": false,
            "description": "Data form ids for the project.",
            "items": {
                "type": "number"
            }
        },
        "stationFormTemplateIds": {
            "type": "array",
            "optional": false,
            "description": "Form template ids for the stations in this project.",
            "items": {
                "type": "number"
            }
        },
        "geometry": {
            "$ref": "geometry"
        },
        "locations": {
            "$ref": "locations"
        }
    }
}