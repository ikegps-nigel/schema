{
    "id": "project",
    "description": "A SPIDAMin project",
    "type": "object",
    "properties": {
        "id": {
            "type": "number"
        },
        "name": {
            "type": "string"
        },
        "statusColor": {
            "type": "string"
        },
        "statusName": {
            "type": "string"
        },
        "requiredFilled": {
            "type": "boolean"
        },
        "projectCodes": {
            "type": "array",
            "description": "Codes for the project.",
            "items": {
                "type": "string",
                "value": "string"
            }
        },
        "formTemplateIds": {
            "type": "array",
            "description": "Form template ids for the project.",
            "items": {
                "type": "number"
            }
        },
        "dataFormIds": {
            "type": "array",
            "description": "Data form ids for the project.",
            "items": {
                "type": "number"
            }
        },
        "stationFormTemplateIds": {
            "type": "array",
            "description": "Form template ids for the stations in this project.",
            "items": {
                "type": "number"
            }
        },
        "geometry": {
            "$ref": "geometry"
        },
        "address": {
            "$ref": "address"
        },
        "stations": {
            "type": "array",
            "items": {
                "id": "location",
                "description": "A SPIDAMin station",
                "type": {
                    "$ref": "station"
                }
            }
        }
    }
}