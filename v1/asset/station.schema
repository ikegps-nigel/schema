{
  "id": "station",
  "description": "A SPIDAMin station",
  "type": "object",
  "properties": {
    "stationId": {
      "type": "string",
      "required": true
    },
    "dataProviderId": {
      "type": "number",
      "required": true,
      "description": "Id of company that provided the asset data"
    },
    "primaryAssetOwnerId": {
      "type": "number",
      "required": true,
      "description": "Id of company that owns the primary asset"
    },
    "type": {
      "required": true,
      "type": "string"
    },
    "assetTypes": {
      "required": true,
      "type": "array",
      "items": {
        "type": "string",
        "required": true,
        "choices": [
          {
            "value": "POLE"
          },
          {
            "value": "NJUNS"
          },
          {
            "value": "ANALYSIS"
          }
        ]
      }
    },
    "geometry": {
      "required": true,
      "$ref": "https://raw.github.com/spidasoftware/schema/master/v1/general/geometry.schema"
    },
    "stationAssets": {
      "$ref": "https://raw.github.com/spidasoftware/schema/master/v1/asset/assets.schema"
    }
  }
}