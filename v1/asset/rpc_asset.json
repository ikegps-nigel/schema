{
    "getName": {
        "description": "Get name of this service.",
        "returns": "string"
    },
    "getPossibleAssetTypes": {
        "description": "Analyze given pole and wait for results",
        "returns": {
            "type": "array",
            "name": "types",
            "required": true,
            "description": "Asset types in this service",
            "items": {
                "type": "string"
            }
        },
        "params": [
            {
                "type": "number",
                "name": "companyId",
                "required": true,
                "description": "Company to get the asset types for."
            }
        ]
    },
    "getStations": {
        "description": "Analyze given pole and wait for results",
        "returns": {
            "$ref": "stations"
        },
        "params": [
            {
                "type": "array",
                "name": "keys",
                "required": true,
                "description": "Keys to return.",
                "items": {
                    "type": "object",
                    "properties": {
                        "key": "string",
                        "value": "string",
                        "operator": {
                            "type": "string",
                            "required": false
                        }
                    }
                }
            },
            {
                "type": "object",
                "name": "bbox",
                "required": false,
                "description": "Whether to fetch station asset details with the query.",
                "properties": {
                    "south": "number",
                    "west": "number",
                    "north": "number",
                    "east": "number"
                }
            },
            {
                "type": "array",
                "name": "foreignCompanyIds",
                "required": false,
                "description": "Keys to return.",
                "items": {
                    "type": "number"
                }
            },
            {
                "type": "boolean",
                "name": "details",
                "required": false,
                "description": "Whether to fetch station asset details with the query."
            }
        ]
    },
    "getAssets": {
        "description": "Analyze given pole and wait for results",
        "returns": {
            "$ref": "assets"
        },
        "params": [
            {
                "type": "array",
                "name": "assetIds",
                "required": true,
                "description": "Keys to return.",
                "items": {
                    "type": "string"
                }
            },
            {
                "type": "boolean",
                "name": "details",
                "required": false,
                "description": "Whether to fetch station asset details with the query."
            }
        ]
    },
    "id": "rpc_asset",
    "description": "This schema allows for the analysis of poles"
}